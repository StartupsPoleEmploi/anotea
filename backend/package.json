{
  "name": "anotea",
  "version": "1.0.0",
  "description": "Anotea : l'advisor de la formation ! Accédez aux avis des anciens stagiaires pour mieux choisir sa formation !",
  "homepage": "https://anotea.pole-emploi.fr",
  "repository": {
    "type": "git",
    "url": "git@github.com:StartupsPoleEmploi/anotea.git"
  },
  "license": "ISC",
  "author": "Loïc Guillois <loic@fitlab.fr>",
  "contributors": [
    "Loïc Guillois <loic@fitlab.fr>",
    "Benoît Guérout <bguerout@yonoma.com>",
    "Nabil Choukri <nabil.choukri@pole-emploi.fr>"
  ],
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p build/public/static/js/widget/ && uglifyjs src/http/public/static/js/widget/anotea-widget-loader.js --source-map --output build/public/static/js/widget/anotea-widget-loader.min.js",
    "lint": "eslint src test",
    "smtp:start": "docker rm smtp; docker run -d --name smtp -p 1025:1025 -p 8025:8025 mailhog/mailhog",
    "smtp:stop": "docker stop smtp",
    "start": "node index.js",
    "start:watch": "nodemon --inspect --ignore test/ index.js",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "nyc --reporter=html --reporter=text-summary mocha --recursive --exit --timeout 5000 test",
    "test:integration": "mocha --recursive --exit --timeout 5000 test/integration",
    "test:unit": "mocha --recursive --exit --timeout 1000 test/unit"
  },
  "dependencies": {
    "@sentry/node": "5.10.2",
    "@slack/webhook": "5.0.2",
    "basic-auth": "2.0.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-prettystream": "0.1.3",
    "byline": "5.0.0",
    "colors": "1.4.0",
    "commander": "4.0.1",
    "compression": "1.7.4",
    "config": "3.2.4",
    "cookie-parser": "1.4.4",
    "csv-parse": "4.8.2",
    "deep-object-diff": "1.1.0",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "faker": "4.1.0",
    "fluent-logger": "3.4.0",
    "iconv-lite": "0.5.0",
    "isemail": "3.2.0",
    "joi": "14.3.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "md5": "2.2.1",
    "md5-file": "4.0.0",
    "mjml": "4.5.1",
    "mobile-detect": "1.4.4",
    "moment": "2.24.0",
    "mongodb": "3.4.0",
    "node-mailjet": "3.3.1",
    "node-uuid": "1.4.8",
    "nodemailer": "6.4.2",
    "nodemailer-html-to-text": "3.1.0",
    "openid-client": "3.10.0",
    "remove-accents": "0.4.2",
    "rotating-file-stream": "1.4.3",
    "stream-transform": "2.0.1",
    "string": "3.3.3",
    "swagger-ui-express": "4.1.2",
    "underscore.string": "3.3.5",
    "uuid": "3.3.3",
    "xml2js": "0.4.22",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "eslint": "6.7.2",
    "mocha": "6.2.2",
    "nodemon": "2.0.2",
    "nyc": "14.1.1",
    "supertest": "4.0.2",
    "uglify-js": "3.7.2",
    "wait-until": "0.0.2"
  },
  "engines": {
    "node": ">= 10.14.1"
  }
}
