{
  "env": "ANOTEA_ENV",
  "app": {
    "public_hostname": "ANOTEA_APP_PUBLIC_HOSTNAME",
    "session_secret": "ANOTEA_APP_SESSION_SECRET"
  },
  "mongodb": {
    "uri": "ANOTEA_MONGODB_URI"
  },
  "security": {
    "secret": "ANOTEA_SECURITY_SECRET"
  },
  "auth": {
    "export": {
      "secret": "ANOTEA_AUTH_EXPORT_SECRET"
    },
    "backoffice": {
      "secret": "ANOTEA_AUTH_BACKOFFICE_SECRET"
    },
    "esd": {
      "secret": "ANOTEA_AUTH_ESD_SECRET"
    },
    "kairos": {
      "secret": "ANOTEA_AUTH_KAIROS_SECRET"
    },
    "maformation": {
      "secret": "ANOTEA_AUTH_MAFORMATION_SECRET"
    }
  },
  "log": {
    "level": "ANOTEA_LOG_LEVEL",
    "type": "ANOTEA_LOG_TYPE",
    "fluentbit": {
      "host": "ANOTEA_LOG_FLUENTBIT_HOST",
      "port": "ANOTEA_LOG_FLUENTBIT_PORT"
    },
    "datalake": {
      "path": "ANOTEA_LOG_DATALAKE_PATH"
    }
  },
  "peconnect": {
    "client_id": "ANOTEA_PECONNECT_CLIENT_ID",
    "client_secret": "ANOTEA_PECONNECT_CLIENT_SECRET",
    "callback_url": "ANOTEA_PECONNECT_CLIENT_CALLBACK_URL",
    "auth_base_url": "ANOTEA_PECONNECT_AUTH_BASE_URL",
    "api_base_url": "ANOTEA_PECONNECT_API_BASE_URL",
    "timeout": "ANOTEA_PECONNECT_TIMEOUT"
  },
  "sentry": {
    "dsn": "ANOTEA_SENTRY_DSN"
  },
  "analytics": {
    "ga": "ANOTEA_GOOGLE_ANALYTICS_ID"
  },
  "smtp": {
    "host": "ANOTEA_SMTP_HOST",
    "port": "ANOTEA_SMTP_PORT",
    "user": "ANOTEA_SMTP_USER",
    "password": "ANOTEA_SMTP_PASSWORD"
  },
  "front": {
    "googleSiteVerification": "ANOTEA_FRONT_GOOGLE_SITE_VERIFICATION"
  },
  "slack": {
    "webhookUrl": "ANOTEA_SLACK_WEBHOOK_URL"
  }
}
